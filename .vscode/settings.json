{
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontSize": 15,
  "terminal.integrated.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": true
}
{
  "workbench.colorCustomizations": {
    "[Default Dark+]": {
      "editor.background": "#1e1f22",
      "sideBar.background": "#2b2d30",
      "tab.activeBackground": "#3C3F41",
      "activityBar.background": "#3C3F41",
      "statusBar.background": "#3C3F41",
      "terminal.background": "#2B2B2B",
      "editorLineNumber.foreground": "#606366"
    }
  }
}
{
  "editorBracketHighlight.foreground1": "#E6B773",
  "editorBracketHighlight.foreground2": "#8AAD7E",
  "editorBracketHighlight.foreground3": "#5394EC",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true
}
